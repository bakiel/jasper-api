# JASPER API (Contact Form) - Environment Variables
# Copy this file to .env and fill in the values

# Database
DATABASE_URL=postgresql://user:password@host:5432/jasper_api

# Security
SECRET_KEY=your-secret-key-generate-with-openssl-rand-hex-32

# Debug Mode (set to false in production)
DEBUG=false

# CORS Allowed Origins
ALLOWED_ORIGINS=https://jasperfinance.org,https://www.jasperfinance.org
